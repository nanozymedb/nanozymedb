<%-include("../partials/header") %> <%-include("../partials/message") %>

<div class="search-table container-overall">
  <% if (flaggedEntry.length<1) { %>
  <p>There are no flagged entries</p>
  <% } else { %>
  <h1>Flagged Entries</h1>
  <table class="table table-condensed table-responsive">
    <thead>
      <tr>
        <th class="table-dark"><h5>Flagged Nanozyme</h5></th>
        <th class="table-dark"><h5>Change Raised</h5></th>
        <th class="table-dark"><h5>Date</h5></th>
        <th class="table-dark"><h5>More Info.</h5></th>
      </tr>
    </thead>
    <tbody class="panel">
      <% flaggedEntry.forEach(flaggedEntry=>{ %>
      <tr>
        <td class="table-dark">
          <p><%= flaggedEntry.flaggedNanozymeName %></p>
        </td>
        <td class="table-dark"><p><%= flaggedEntry.changeRaised %></p></td>
        <td class="table-dark"><p><%= flaggedEntry.date %></p></td>
        <td class="table-dark">
          <a href="/editor/flagged-entry-details/<%= flaggedEntry._id %>"
            ><i class="fas fa-info info-icon fa-sm"></i
          ></a>
        </td>
      </tr>
      <tr></tr>
      <% }) %>
    </tbody>
  </table>
  <% if (pages > 0) { %><% if (entries >20) { %>
  <nav aria-label="...">
    <ul class="pagination justify-content-center">
      <% if (current == 1) { %>
      <li class="page-item disabled"><a class="page-link">First</a></li>
      <% } else { %>
      <li><a class="page-link" href="/editor/flagged-entries">First</a></li>
      <% } %> <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %> <%
      if (i !== 1) { %>
      <li class="page-item disabled"><a class="page-link">...</a></li>
      <% } %> <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %> <%
      if (i == current) { %>
      <li class="page-item active"><a class="page-link"><%= i %></a></li>
      <% } else { %>
      <li>
        <a class="page-link" href="/editor/flagged-entries?page=<%= i %>"
          ><%= i %>
        </a>
      </li>
      <% } %> <% if (i == Number(current) + 4 && i < pages) { %>
      <li class="page-item disabled"><a class="page-link">...</a></li>
      <% } %> <% } %> <% if (current == pages) { %>
      <li class="page-item disabled"><a class="page-link">Last</a></li>
      <% } else { %>
      <li>
        <a class="page-link" href="/editor/flagged-entries?page=<%= pages %>"
          >Last</a
        >
      </li>
      <% } %>
    </ul>
    <% } %> <% } %>
  </nav>

  <% } %>
</div>
<%-include("../partials/footer") %>
